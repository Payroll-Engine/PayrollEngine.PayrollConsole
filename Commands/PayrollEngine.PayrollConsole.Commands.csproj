<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<AssemblyName>PayrollEngine.PayrollConsole.Commands</AssemblyName>
		<RootNamespace>PayrollEngine.PayrollConsole.Commands</RootNamespace>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<Description>Payroll Engine Console command library</Description>
	</PropertyGroup>

	<!-- help file generation -->
	<PropertyGroup>
		<DocumentationFile>$(AssemblyName).xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="PayrollEngine.Client.Services" Version="0.9.0-beta.14" />
		<PackageReference Include="NPOI" Version="2.7.5" />
		<PackageReference Include="PayrollEngine.Document" Version="0.9.0-beta.14" />
	</ItemGroup>
</Project>
